{
  "name": "hardhat-project",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "hardhat": "^2.26.3"
  },
  "dependencies": {
    "@chainlink/contracts": "^1.5.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0"
  },
  "scripts": {
    "deploy:sepolia": "node --env-file .env $(which hardhat) run scripts/deploy-testnet.js --network sepolia",
    "verify:nft": "node --env-file .env $(which hardhat) verify --network sepolia 0x1F761a7F0cEE38d4Df3729ec572De0fD0Edd4A96",
    "verify:auction": "node --env-file .env $(which hardhat) verify --network sepolia 0x3f198f869805B216edD543E34E196db3fF2F4d71"
  }
}


